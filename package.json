{
  "name": "angular-truffle-dapp",
  "productName": "Seths Network",
  "version": "1.0.0",
  "description": "Seths Network for Final Degree Project about Scalability Issues in the Blockchain",
  "descriptions": "Seths Network for Final Degree Project about Scalability Issues in the Blockchain",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "truffle": "truffle",
    "compile-eth": "truffle compile",
    "migrate-eth": "truffle migrate",
    "electron": "electron .",
    "electron-build": "ng build --prod && electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . seths-network --overwrite --asar=true --platform=win32 --arch=ia32 --icon=dist/angular-truffle-dapp/favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Seths Network\"",
    "package-linux": "electron-packager . seths-network --overwrite --asar=true --platform=linux --arch=x64 --icon=dist/angular-truffle-dapp/favicon.ico --prune=true --out=release-builds",
    "create-win-installer": "node installers/windows/createinstaller.js",
    "create-installer-mac": "electron-installer-dmg ./release-builds/Seths\\ Network-darwin-x64/Seths\\ Network.app seths-network-app --out=release-builds --overwrite",
    "create-debian-installer": "electron-installer-debian --src release-builds/seths-network-linux-x64/ --arch amd64 --config debian.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/cdk": "^6.1.0",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/material": "^6.1.0",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/router": "^6.0.2",
    "async-mutex": "^0.1.3",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.4",
    "electron": "^2.0.1",
    "nedb": "^1.8.0",
    "rxjs": "^6.0.0",
    "web3": "^1.0.0-beta.34",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular/cli": "~6.0.3",
    "@angular/compiler-cli": "^6.0.2",
    "@angular/language-service": "^6.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "codelyzer": "~4.2.1",
    "electron-installer-dmg": "^1.0.0",
    "electron-winstaller": "^2.6.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "truffle-contract": "^3.0.5",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
